<form method="post" action="{{ path('objet_delete', {'id': objet.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ objet.id) }}">
    <button class="btn btn-danger" style='margin-top:10px'>Supprimer</button>
</form>
